#include <stdio.h>
#include <stdlib.h>

int main(){

	int var1 = 5, var2 = 10, i;
    int *ptr1, *ptr2;

	ptr1 = &var1;
	ptr2 = &var2;
	
	printf("Valores das variaveis: ptr1 = %d e ptr2 = %d\n", *ptr1, *ptr2);
	
	i = *ptr1;
	*ptr1 = *ptr2;
	*ptr2 = i;
	
	printf("Os novos valores sao: \nptr1 = %d\n ptr2 = %d\n", *ptr1, *ptr2);
    

    return 0;
}
